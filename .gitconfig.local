[alias]
  tags = tag --list
  remotes = remote -v
  branches = branch -a
  stashes = stash list
  summary = status -u -s
  amend = commit --amend
  graph = log --graph --oneline --decorate
  fullgraph = log --graph --oneline --decorate --all
  history = log -10 --format=format:'%Cgreen%h %Cresetâ€¢ %s (%cN, %ar)'

  su = submodule
  suu = submodule update
  sua = submodule add
[rebase]
  autosquash = true
  autostash = true
