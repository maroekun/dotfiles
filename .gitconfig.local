[alias]
    logg = log --graph
    logo = log --oneline
    loggg = log --pretty=format:'%h %s' --graph
    logog = log --oneline --graph
    st = status
    sh = show
    di = diff
    dic = diff --cached
    ci = commit -a
    co = checkout
    b = branch
    ba = branch -a
    su = submodule
    suu = submodule update
    sua = submodule add
