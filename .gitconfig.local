[alias]
  logg = log --graph
  logo = log --oneline
  loggg = log --pretty=format:'%h %s' --graph
  log2 = log --graph --branches --pretty=format:'%d [%h] \"%s\"'
  logog = log --oneline --graph
  st = status
  sh = show
  di = diff
  dic = diff --cached
  ci = commit -a
  co = checkout
  b = branch
  ba = branch -a
  su = submodule
  suu = submodule update
  sua = submodule add
